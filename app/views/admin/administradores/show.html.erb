<div class="container mt-5 mb-3">
  <div class="row">
    <div class="col">
      <h1>Mostrar Administrador</h1>
    </div>
    <div class="col text-end">
      <%= link_to "Voltar", admin_administradores_path, class: "btn btn-dark" %>
      <%= link_to "Editar", edit_admin_administrador_path(@administrador), class: "btn btn-warning" %>
    </div>
  </div>
  <div id="<%= dom_id @administrador %>">
    <p>
      <strong>Nome:</strong>
      <%= @administrador.nome %>
    </p>
    <p>
      <strong>Email:</strong>
      <%= @administrador.email %>
    </p>
    <p>
      <strong>Senha:</strong>
      <%= @administrador.senha %>
    </p>
  </div>
</div>
